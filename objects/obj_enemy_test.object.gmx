            <string>/// Damage the player
//If the enemy isn't frozen after attack
if (state != scr_enemy_stale_state) {
    //Get the direction of the other object
    var dir = point_direction(other.x, other.y, x, y);
    var xdir = lengthdir_x(1, dir);
    var ydir = lengthdir_y(1, dir);
    //Create a damage box ontop of the other character
    var damage = instance_create(other.x+xdir, other.y+ydir, obj_damage);
    damage.creator = id;
    //Change knockback to large number to add distance for player after damage
    damage.knockback = 32500;
    //Freeze enemy after attacking and set alarm to speed of room
    state = scr_enemy_stale_state;
    alarm[1] = room_speed;
}
